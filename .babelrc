{
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    ["babel-plugin-transform-inline-environment-variables", {
      "include": [ "NODE_ENV" ]
    }]
  ],
  "env":{
    "es": {
      "presets": [
        ["babel-preset-env", {
          "modules": false,
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ],
      "plugins": [
        "babel-plugin-annotate-pure-calls"
      ]
    },
    "rollup": {
      "presets": [
        ["babel-preset-env",{
          "modules": false,
          "targets":{
            "chrome": 43,
            "firefox": 43,
            "safari": 10,
            "ios": 10,
            "opera": 40,
            "edge": 13,
            "ie": 9
          }
        }]
      ],
      "plugins": ["babel-plugin-external-helpers"]
    }
  }
}
